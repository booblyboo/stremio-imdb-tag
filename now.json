{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@now/node" }
  ],
  "routes": [
    {
        "src": "/(?<list>[^/]*)/(?<sort>[^/]*)/manifest.json",
        "dest": "/manifest.js"
    },
    {
        "src": "/(?<list>[^/]*)/manifest.json",
        "dest": "/manifest.js"
    },
    {
        "src": "/(?<list>[^/]*)/(?<sort>[^/]*)/(?<resource>[^/]*)/(?<type>[^/]*)/(?<id>[^/]*).json",
        "dest": "/$resource.js"
    },
    {
        "src": "/(?<list>[^/]*)/(?<resource>[^/]*)/(?<type>[^/]*)/(?<id>[^/]*).json",
        "dest": "/$resource.js"
    }
  ]
}
